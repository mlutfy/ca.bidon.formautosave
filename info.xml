<?xml version="1.0"?>
<extension key="ca.bidon.formautosave" type="module">
  <file>formautosave</file>
  <name>CiviCRM form auto-save</name>
  <description>Automatically saves to disk (localstorage) some backend CiviCRM forms to avoid losing data when unexpected errors occur.</description>
  <license>AGPL3+</license>
  <maintainer>
    <author>Mathieu Lutfy</author>
    <email>mathieu@bidon.ca</email>
  </maintainer>
  <releaseDate>2018-01-26</releaseDate>
  <version>1.2</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>4.6</ver>
    <ver>4.7</ver>
  </compatibility>
  <comments>Support available from https://www.SymbioTIC.coop</comments>
  <urls>
    <url desc="Main Extension Page">https://github.com/mlutfy/ca.bidon.formautosave</url>
    <url desc="Documentation">https://github.com/mlutfy/ca.bidon.formautosave/blob/master/README.md</url>
    <url desc="Support">https://www.SymbioTIC.coop/en</url>
    <url desc="Licensing">https://civicrm.org/licensing</url>
  </urls>
  <civix>
    <namespace>CRM/FormAutoSave</namespace>
  </civix>
</extension>
